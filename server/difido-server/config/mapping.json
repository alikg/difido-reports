{
	"settings": {
		"index": {
			"number_of_shards": 2,
			"number_of_replicas": 1
		}
	},
	"mappings": {
		"test": {
			"properties": {
				"name": {
					"type": "keyword"
				},
				"uid": {
					"type": "keyword"
				},
				"parent": {
					"type": "keyword"
				},
				"status": {
					"type": "keyword"
				},
				"url": {
					"type": "keyword"
				},
				"description": {
					"type": "keyword"
				},
				"execution": {
					"type": "keyword"
				},
				"executionId": {
					"type": "long"
				},
				"executionDuration": {
					"type": "long"
				},
				"machine": {
					"type": "keyword"
				},
				"timestamp": {
					"type": "date",
					"format": "yyyy/MM/dd HH:mm:ss"
				},
				"executionTimestamp": {
					"type": "date",
					"format": "yyyy/MM/dd HH:mm:ss"
				},
				"duration": {
					"type": "long"
				}
			},
			"dynamic_templates": [
				{
					"long_prop": {
						"match": "long_*",
						"match_mapping_type": "keyword",
						"mapping": {
							"type": "long"
						}
					}
				},
				{
					"int_prop": {
						"match": "int_*",
						"match_mapping_type": "keyword",
						"mapping": {
							"type": "integer"
						}
					}
				},
				{
					"double_prop": {
						"match": "double_*",
						"match_mapping_type": "keyword",
						"mapping": {
							"type": "double"
						}
					}
				},
				{
					"date_prop": {
						"match": "date_*",
						"match_mapping_type": "keyword",
						"mapping": {
							"type": "date",
							"format": "yyyy/MM/dd HH:mm:ss"
						}
					}
				},
				{
					"notanalyzed": {
						"match": "*",
						"unmatch": "^(long_|int_|double_|date_)$",
						"match_mapping_type": "keyword",
						"mapping": {
							"type": "keyword"
						}
					}
				}
			]
		}
	}
}
